{
	"manifest_version": 3,
	"name": "AI as Workers in India",
	"version": "1.0",
	"description": "Replaces the text 'AI' with what it really means.",
	"permissions": [
		"activeTab",
		"scripting"
	],
	"host_permissions": [
		"*://*/*"
	],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"32": "images/icon-32.png",
		"64": "images/icon-64.png",
		"128": "images/icon-128.png"
	}

}

